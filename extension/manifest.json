{
  "manifest_version": 2,

  "name": "CovidFilter",
  "description": "This extension will highlite misinformation related to covid19",
  "version": "1.0.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],

      "js": ["./js/content.js"]
    }
  ],

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
  ]
}